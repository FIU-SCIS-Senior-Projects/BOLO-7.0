extends layout


block jumbo
    .jumbotron
        .container
            h3 BOLO Search Results
            input#input.hidden(value='default')


block content
    if bolos && 0 === bolos.length
        p.lead No bolos were found matching the specified criteria.
    else
        .container
            #bolo-list.row.text-center
                include partials/bolo-thumbnails

