if (bolos.length)
    ul.pagination.pagination-lg
        if ( 1 === paging.current )
            li.disabled: a( href="#" aria-label="Previous" )
                span.glyphicon.glyphhicon-menu-left( aria-hidden="true" )
        else
            li: a( href='/bolo/' + '?page=' + ( paging.current - 1 ) aria-label="Previous" )
                span.glyphicon.glyphicon-menu-left( aria-hidden="true" )

        - var page = 1;
        while page <= paging.last
            if page === paging.current
                li.active: a( href="#" ) #{page}
                    span.sr-only (current)
            else
                li: a( href='/bolo/' + '?page=' + page) #{page}

            - page = page + 1

        if ( paging.last === paging.current )
            li.disabled: a( href="#" aria-label="Next" )
                span.glyphicon.glyphhicon-menu-right( aria-hidden="true" )
        else
            li: a(href='/bolo/' + '?page=' + ( paging.current + 1 )  aria-label="Next" )
                span.glyphicon.glyphicon-menu-right( aria-hidden="true" )

else
    p.text-small No Paging Available
